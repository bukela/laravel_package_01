{
    "name": "mirko/dbase",
    "type": "library",
    "description": "package test",
    "keywords": ["demo"],
    "homepage": "https://gitlab.com/dinfi/demo",
    "license": "proprietary",
    "authors": [
      {
        "name": "mirko",
        "email": "bukela@gmail.com",
        "homepage": "https://mirko.com",
        "role": "developer"
      }
    ],
    "require": {
      "php": "^7.1.3"
    },
    "require-dev": {
      "phpunit/phunit": ">=5.4.3"
    },
    "autoload": {
      "psr-4": {
        "Mirko\\Dbase\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Mirko\\Dbase\\": "tests"
      }
    },
    "extra": {
          "laravel": {
              "providers": [
                  "Mirko\\Dbase\\DbaseServiceProvider"
              ],
              "aliases": {
                  "Dbase": "Mirko\\Dbase\\DbaseFacade"
              }
          }
      },
      "prefer-stable": true,
      "minimum-stability": "dev",
      "config": {
          "sort-packages": true
      }
  }
  